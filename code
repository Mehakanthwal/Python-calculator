print("enter first number")
num1= int(input())
print ("enter second number")
num2=int(input())
operator= input (("choice an operator(+,-,*,/):"))
if operator=="+":
    print(num1+num2)
elif operator=="-":
    print(num1-num2)
elif operator=="*":
    print(num1*num2)
elif operator=="/":
    print(num1/num2)
else:
    print("invalid operation")
